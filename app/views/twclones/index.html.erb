<h1>Tweet List</h1>
<p><%= notice %></p>
<table>
  <tr>
    <th>Content</th>
  </tr>
<% @twclones.each do |twclone| %>
  <tr>
    <td><%= twclone.content %></td>
    <td><%= link_to 'edit tweet', edit_twclone_path(twclone.id) %></td>
    <td><%= link_to 'delete tweet', twclone_path(twclone.id), method: :delete, data: { confirm: 'sure to delete tweet?'} %></td>
  </tr>
<% end %>
</table>
<%= link_to 'create new tweet', new_twclone_path %>